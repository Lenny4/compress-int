{
    "name": "lenny4/compress-int",
    "description": "Compress integer into alphanumeric",
    "keywords": [
        "Lenny4",
        "compress-int"
    ],
    "homepage": "https://github.com/lenny4/compress-int",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Beaujour",
            "email": "alexandre.beaujour@hotmail.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Lenny4\\CompressInt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lenny4\\CompressInt\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
